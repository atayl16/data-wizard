<h1 id="title">Salon Services</h1>
<a class="btn btn-primary" href="salonservices.csv" role="button">Export</a>

<table class="table table-striped table-bordered table-sm" cellspacing="0">
  <thead>
    <tr>
      <th>service_id</th>
      <th>category_id</th>
      <th>name</th>
      <th>description</th>
      <th>duration</th>
      <th>is_add_on</th>
      <th>is_custom</th>
      <th>breed_group_id</th>
      <th>sku</th>
      <th>deleted</th>
    </tr>
  </thead>

  <tbody>
    <% @salonservices.each do |salonservice| %>
      <tr>
        <td><%= salonservice.service_id %></td>
        <td><%= salonservice.category_id %></td>
        <td><%= salonservice.name %></td>
        <td><%= salonservice.description %></td>
        <td><%= salonservice.duration %></td>
        <td><%= salonservice.is_add_on %></td>
        <td><%= salonservice.is_custom %></td>
        <td><%= salonservice.breed_group_id %></td>
        <td><%= salonservice.sku %></td>
        <td><%= salonservice.deleted %></td>
      </tr>
    <% end %>
  </tbody>
</table>
