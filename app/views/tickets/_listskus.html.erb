<a class="btn btn-danger" href="tickets/export_skus.csv" role="button">Export SKUs</a>

<table class="table table-striped table-bordered table-sm" cellspacing="0">
  <thead>
    <tr>
      <th>Ticket</th>
      <th>Inventory SKU</th>
      <th>Quantity</th>
      <th>Amount before discount</th>
      <th>SKU Amount</th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.ticket_id %></td>
        <td><%= ticket.inventory_sku_id %></td>
        <td><%= ticket.quantity %></td>
        <td><%= ticket.amount_before_discount %></td>
        <td><%= ticket.skuamount %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= will_paginate @tickets, renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>
