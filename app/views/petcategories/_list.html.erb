<a class="btn btn-outline-primary" href="petcategories.csv" role="button">Export</a>
<table class="table table-striped table-bordered table-sm" cellspacing="0">
  <thead>
    <tr>
      <th>Category</th>
      <th>Name</th>
      <th>Online</th>
      <th>Deleted</th>
    </tr>
  </thead>

  <tbody>
    <% @petcategories.each do |petcategory| %>
      <tr>
        <td><%= petcategory.category_id %></td>
        <td><%= petcategory.name %></td>
        <td><%= petcategory.online %></td>
        <td><%= petcategory.deleted %></td>
      </tr>
    <% end %>
  </tbody>
</table>
